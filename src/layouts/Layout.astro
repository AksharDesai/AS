<script is:inline>
 
// Simulate page load progress
let progress = 0;
const progressElement = document.querySelector('.progress');
const loaderWrapper = document.querySelector('.loader-wrapper');

const progressInterval = setInterval(() => {
  progress++;
  progressElement.style.width = progress + '%';

  if (progress >= 100) {
    clearInterval(progressInterval);
    // Wait for 1 second after reaching 100%
    setTimeout(() => {
      loaderWrapper.style.opacity = 0;
      // Optionally remove loader from DOM after fade-out
      setTimeout(() => {
        loaderWrapper.style.display = 'none';
      }, 500); // Matches CSS transition duration
    }, 350);
  }
}); // Adjust interval as needed (30ms per increment gives ~3 seconds total)

 
document.addEventListener('DOMContentLoaded', () => {
  const currentPath = window.location.pathname.replace(/\/$/, '');
  const navLinks = document.querySelectorAll('#nav-links a');

  if (navLinks.length === 0) {
    console.warn('No navigation links found with id "nav-links"');
    return;
  }

  navLinks.forEach(link => {
    const normalizedLinkPath = link.pathname.replace(/\/$/, '');
    const isActive = normalizedLinkPath === currentPath;

    link.classList.toggle('bg-neutral-800', isActive);
    link.classList.toggle('text-red-500', isActive);
    link.setAttribute('aria-current', isActive ? 'page' : '');
  });
});



  //  Check and prompt login modal every 10 seconds if not logged in
   setInterval(() => {
          const userPhone = localStorage.getItem("userPhone");
          const loginModal = document.getElementById('Login_modal');
          const isModalOpen = loginModal && !loginModal.classList.contains("hidden") && !loginModal.classList.contains("opacity-0");

          if (!userPhone && !isModalOpen) {
            const loginButton = document.querySelector(".openLoginModal");
            if (loginButton) {
              loginButton.click();
            }
          }
        }, 30000);
         // 10000 milliseconds = 10 seconds
      




</script>

